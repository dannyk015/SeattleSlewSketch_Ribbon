							<ComboBoxItem IsSelected="True" Content="Use Settings"/>
							<ComboBoxItem Content="Never Reinvest"/>
						</ComboBox>
						<TextBlock Grid.Row="2" HorizontalAlignment="Left" TextWrapping="Wrap" Text="Non Reinvesting Funds"/>
