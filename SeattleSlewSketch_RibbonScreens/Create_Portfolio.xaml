						<StackPanel Grid.Row="3" Grid.Column="1" HorizontalAlignment="Center" Orientation="Horizontal">
							<Button Content="Add" Grid.Row="2" Grid.Column="2" HorizontalAlignment="Left" Width="75" Margin="5"/>
